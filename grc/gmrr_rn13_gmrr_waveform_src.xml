<?xml version="1.0"?>
<block>
  <name>gmrr_waveform_src</name>
  <key>gmrr_rn13_gmrr_waveform_src</key>
  <category>gmrr_rn13</category>
  <import>import gmrr_rn13</import>
  <make>gmrr_rn13.gmrr_waveform_src($filename)</make>
  <callback>set_filename($filename)</callback>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>filename</name>
    <key>filename</key>
    <type>file_open</type>
  </param>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
